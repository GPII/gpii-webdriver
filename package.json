  "dependencies": {
    "infusion": "2.0.0-dev.20160519T222603Z.754d2c6",
    "selenium-webdriver": "2.53.3",
    "webdriver-manager": "10.2.1"
  },
  "devDependencies": {
    "eslint": "3.1.1",
    "grunt": "1.0.1",
    "grunt-eslint": "18.1.0",

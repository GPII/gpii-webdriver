{
    "sources": {
        "md":    [ "./*.md", "./docs/*.md"],
        "js":    ["./src/**/*.js", "./tests/**/*.js", "./*.js"],
        "json":  ["src/**/*.json", "tests/**/*.json", "./*.json"],
        "json5": [],
        "other": ["./.*", "!./package-lock.json"]
    },
    "eslint": {
        "md": {
